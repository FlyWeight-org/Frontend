{
  "name": "flyweight",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "start-server-and-test preview http://127.0.0.1:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://127.0.0.1:4173 'cypress open --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint .",
    "format": "prettier --write src/",
    "stylelint": "stylelint \"src/**/*.{vue,css,scss,sass}\""
  },
  "dependencies": {
    "@bugsnag/browser-performance": "^2.6.0",
    "@bugsnag/js": "^7.23.0",
    "@bugsnag/plugin-vue": "^7.22.7",
    "@vuepic/vue-datepicker": "^8.8.0",
    "actioncable": "5.2.8-1",
    "date-fns": "^3.6.0",
    "is-touch-device": "^1.0.1",
    "lodash-es": "^4.17.21",
    "luxon": "^3.4.4",
    "normalize.css": "^8.0.1",
    "pinia": "^2.1.7",
    "ts-results": "^3.3.0",
    "vue": "^3.4.29",
    "vue-i18n": "^9.13.1",
    "vue-router": "^4.3.3"
  },
  "devDependencies": {
    "@flydotio/dockerfile": "^0.5.7",
    "@pinia/testing": "^0.1.3",
    "@rushstack/eslint-patch": "^1.10.3",
    "@tsconfig/node20": "^20.1.4",
    "@types/actioncable": "^5.2.11",
    "@types/is-touch-device": "^1.0.2",
    "@types/jsdom": "^21.1.7",
    "@types/lodash-es": "^4.17.12",
    "@types/luxon": "^3.4.2",
    "@types/mailparser": "^3.4.4",
    "@types/node": "^20.14.6",
    "@types/setimmediate": "^1.0.4",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "cypress": "^13.12.0",
    "eslint": "^9.5.0",
    "eslint-plugin-cypress": "^3.3.0",
    "eslint-plugin-vue": "^9.26.0",
    "jsdom": "^24.1.0",
    "mailparser": "^3.7.1",
    "npm-run-all2": "^6.2.0",
    "postcss": "^8.4.38",
    "postcss-html": "^1.7.0",
    "prettier": "^3.3.2",
    "sass": "^1.77.6",
    "setimmediate": "^1.0.5",
    "start-server-and-test": "^2.0.4",
    "stylelint": "^16.6.1",
    "stylelint-config-recess-order": "^5.0.1",
    "stylelint-config-recommended-scss": "^14.0.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "typescript": "^5.4.5",
    "vite": "^5.3.1",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.0.21"
  },
  "packageManager": "yarn@4.3.0"
