{
  "name": "flyweight",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=25.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "start-server-and-test 'vite preview --host 127.0.0.1 --port 4173' http://127.0.0.1:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --host 127.0.0.1 --port 4173' http://127.0.0.1:4173 'cypress open --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint:eslint": "eslint . --fix",
    "lint:stylelint": "stylelint \"src/**/*.{vue,css,scss,sass}\"",
    "lint": "run-s 'lint:*'",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@intlify/core-base": "^11.2.8",
    "@rails/actioncable": "^8.1.200",
    "@sentry/vue": "^10.38.0",
    "@vuepic/vue-datepicker": "^12.1.0",
    "date-fns": "^4.1.0",
    "is-touch-device": "^1.0.1",
    "lodash-es": "^4.17.23",
    "luxon": "^3.7.2",
    "normalize.css": "^8.0.1",
    "pinia": "^3.0.4",
    "ts-results": "^3.3.0",
    "tslib": "^2.8.1",
    "vue": "^3.5.28",
    "vue-i18n": "^11.2.8",
    "vue-router": "^5.0.2"
  },
  "devDependencies": {
    "@flydotio/dockerfile": "^0.7.10",
    "@pinia/testing": "^1.0.3",
    "@rushstack/eslint-patch": "^1.15.0",
    "@testing-library/cypress": "^10.1.0",
    "@testing-library/vue": "^8.1.0",
    "@tsconfig/node22": "^22.0.5",
    "@types/is-touch-device": "^1.0.3",
    "@types/jsdom": "^27.0.0",
    "@types/lodash-es": "^4.17.12",
    "@types/luxon": "^3.7.1",
    "@types/node": "^25.2.3",
    "@types/rails__actioncable": "^8.0.3",
    "@types/setimmediate": "^1.0.4",
    "@typescript-eslint/utils": "^8.55.0",
    "@vitejs/plugin-vue": "^6.0.4",
    "@vitest/eslint-plugin": "^1.6.7",
    "@vue/compiler-dom": "^3.5.28",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/server-renderer": "^3.5.28",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "cypress": "^15.10.0",
    "cypress-vite": "^1.8.0",
    "eslint": "^9.39.2",
    "eslint-plugin-cypress": "^5.3.0",
    "eslint-plugin-vue": "^10.7.0",
    "jsdom": "^28.0.0",
    "npm-run-all2": "^8.0.4",
    "postal-mime": "^2.7.3",
    "postcss": "^8.5.6",
    "postcss-html": "^1.8.1",
    "prettier": "^3.8.1",
    "sass": "^1.97.3",
    "setimmediate": "^1.0.5",
    "start-server-and-test": "^2.1.3",
    "stylelint": "^17.3.0",
    "stylelint-config-recess-order": "^7.6.1",
    "stylelint-config-recommended-scss": "^17.0.0",
    "stylelint-config-recommended-vue": "^1.6.1",
