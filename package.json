{
  "packageManager": "yarn@4.10.3",
  "name": "flyweight",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "start-server-and-test 'vite preview --host 127.0.0.1 --port 4173' http://127.0.0.1:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --host 127.0.0.1 --port 4173' http://127.0.0.1:4173 'cypress open --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "stylelint": "stylelint \"src/**/*.{vue,css,scss,sass}\""
  },
  "dependencies": {
    "@intlify/core-base": "^11.1.12",
    "@rails/actioncable": "^8.0.300",
    "@sentry/vue": "^10.18.0",
    "@vuepic/vue-datepicker": "^11.0.2",
    "date-fns": "^4.1.0",
    "is-touch-device": "^1.0.1",
    "lodash-es": "^4.17.21",
    "luxon": "^3.7.2",
    "normalize.css": "^8.0.1",
    "pinia": "^3.0.3",
    "ts-results": "^3.3.0",
    "tslib": "^2.8.1",
    "vue": "^3.5.22",
    "vue-i18n": "^11.1.12",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@flydotio/dockerfile": "^0.7.10",
    "@pinia/testing": "^1.0.2",
    "@rushstack/eslint-patch": "^1.13.0",
    "@testing-library/cypress": "^10.1.0",
    "@testing-library/vue": "^8.1.0",
    "@tsconfig/node22": "^22.0.2",
    "@types/is-touch-device": "^1.0.3",
    "@types/jsdom": "^27.0.0",
    "@types/lodash-es": "^4.17.12",
    "@types/luxon": "^3.7.1",
    "@types/node": "^24.7.0",
    "@types/rails__actioncable": "^6.1.11",
    "@types/setimmediate": "^1.0.4",
    "@typescript-eslint/utils": "^8.46.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/eslint-plugin": "^1.3.16",
    "@vue/compiler-dom": "^3.5.22",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/server-renderer": "^3.5.22",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "cypress": "^15.4.0",
    "cypress-vite": "^1.8.0",
    "eslint": "^9.37.0",
    "eslint-plugin-cypress": "^5.2.0",
    "eslint-plugin-vue": "^10.5.0",
    "jsdom": "^27.0.0",
    "npm-run-all2": "^8.0.4",
    "postal-mime": "^2.5.0",
    "postcss": "^8.5.6",
    "postcss-html": "^1.8.0",
    "prettier": "^3.6.2",
    "sass": "^1.93.2",
    "setimmediate": "^1.0.5",
    "start-server-and-test": "^2.1.2",
    "stylelint": "^16.25.0",
    "stylelint-config-recess-order": "^7.3.0",
    "stylelint-config-recommended-scss": "^16.0.2",
    "stylelint-config-recommended-vue": "^1.6.1",
    "stylelint-config-standard-scss": "^16.0.0",
    "stylelint-order": "^7.0.0",
    "typescript": "^5.9.3",
    "vite": "^7.1.9",
    "vite-plugin-vue-devtools": "^8.0.2",
    "vitest": "^3.2.4",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.1.1"
  }
}
