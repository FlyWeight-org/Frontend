{
  "name": "flyweight",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "format": "prettier --write src/",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest --environment jsdom --root src/",
    "build:e2e": "vite preview --mode test",
    "test:e2e": "start-server-and-test build:e2e :4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test build:e2e http://127.0.0.1:4173 'cypress open --e2e'",
    "test:e2e:ci": "start-server-and-test build:e2e http://127.0.0.1:4173 'cypress run --e2e --record --key 10208fb6-d512-42df-bb15-7784c3b14aac'",
    "build-only": "vite build -m production",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@bugsnag/core": "^7.19.0",
    "@bugsnag/js": "^7.20.0",
    "@bugsnag/plugin-vue": "^7.19.0",
    "@intlify/core-base": "^9.2.2",
    "@vuepic/vue-datepicker": "^4.3.0",
    "actioncable": "^5.2.8-1",
    "is-touch-device": "^1.0.1",
    "lodash-es": "^4.17.21",
    "luxon": "^3.3.0",
    "normalize.css": "^8.0.1",
    "pinia": "^2.0.33",
    "ts-results": "^3.3.0",
    "vue": "^3.2.47",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@microsoft/eslint-formatter-sarif": "^3.0.0",
    "@rushstack/eslint-patch": "^1.2.0",
    "@testing-library/cypress": "^9.0.0",
    "@testing-library/vue": "^7.0.0",
    "@types/actioncable": "^5.2.7",
    "@types/is-touch-device": "^1.0.0",
    "@types/jsdom": "^21.1.1",
    "@types/lodash-es": "^4.17.7",
    "@types/luxon": "^3.2.1",
    "@types/mailparser": "^3.4.0",
    "@types/node": "^18.15.11",
    "@types/setimmediate": "^1.0.2",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vue/compiler-sfc": "^3.2.47",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.3.2",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.14",
    "cypress": "^12.9.0",
    "cypress-vite": "^1.3.1",
    "eslint": "^8.37.0",
    "eslint-plugin-cypress": "^2.13.2",
    "eslint-plugin-vue": "^9.10.0",
    "eslint-plugin-vuejs-accessibility": "^2.1.0",
    "jsdom": "^21.1.1",
    "mailparser": "^3.6.4",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "prettier": "^2.8.7",
    "sass": "^1.60.0",
    "setimmediate": "^1.0.5",
    "start-server-and-test": "^2.0.0",
    "typescript": "^5.0.3",
    "vite": "^4.2.1",
    "vite-plugin-node-polyfills": "^0.7.0",
    "vitest": "^0.29.8",
    "vue-tsc": "^1.2.0"
  },
  "packageManager": "yarn@3.4.1"
}
